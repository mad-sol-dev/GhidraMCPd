{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "current_program.v1",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "domain_file_id": {
      "type": "string"
    },
    "locked": {
      "type": "boolean"
    },
    "state": {
      "type": "string",
      "enum": ["IDLE", "LOADING", "READY"]
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "domain_file_id",
    "locked",
    "state"
  ]
}
